{
    "compilerOptions": {
      "target": "es6", 
      "module": "commonjs", 
      "jsx": "react", 
      "strict": true, 
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "outDir": "./dist",
      "rootDir": "./src"
    },
    "include": ["src"]
  }
  